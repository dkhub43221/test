local Players = game:GetService("Players")
LocalPlayer = Players.LocalPlayer
local selectedPlayerName = "None"

local Dropdown
local function getPlayerNames()
    local playerNames = {"None"}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerNames, player.Name)
    end
    return playerNames
end

local function updateDropdown()
    if Dropdown then
        Dropdown.Options = getPlayerNames()
    end
end


Tab:AddButton({
    Name = "Refresh Player List",
    Callback = function()
        updateDropdown()
    end    
})


Dropdown = Tab:AddDropdown({
    Name = "Select Player",
    Default = "None",
    Options = getPlayerNames(),
    Callback = function(Value)
        selectedPlayerName = Value
    end    
})


local function getPlayerInventory(player)
    if player and player:FindFirstChild("Backpack") then
        return player.Backpack:GetChildren()
    end
    return {}
end

local function getPlayerMoney(player)
    if player and player:FindFirstChild("stats") and player.stats:FindFirstChild("Money") then
        return player.stats.Money.Value
    end
    return 0
end

local function getPlayerBounty(player)
    if player and player:FindFirstChild("stats") and player.stats:FindFirstChild("Bounty") then
        return player.stats.Bounty.Value
    end
    return 0
end

local function getPlayerExperience(player)
    if player and player:FindFirstChild("PlayerData") and player.PlayerData:FindFirstChild("Experience") then
        return player.PlayerData.Experience.Value
    end
    return 0
end

local function getPlayerLevel(player)
    if player and player:FindFirstChild("PlayerData") and player.PlayerData:FindFirstChild("Level") then
        return player.PlayerData.Level.Value
    end
    return 0
end


Section = Tab:AddSection({
	Name = "Spys"
})

Tab:AddButton({
    Name = "Show Inventory",
    Callback = function()
        local selectedPlayer = Players:FindFirstChild(selectedPlayerName)
        if selectedPlayer and selectedPlayerName ~= "None" then
            local inventoryItems = getPlayerInventory(selectedPlayer)
            local itemList = {}
            for _, item in ipairs(inventoryItems) do
                if item.Name ~= "Tools" and item.Name ~= "LocalFootsteps" then
                    table.insert(itemList, item.Name)
                end
            end
            local itemsString = table.concat(itemList, ", ") or "No items in backpack."
            OrionLib:MakeNotification({
                Name = selectedPlayer.Name,
                Content = itemsString,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "No player selected or player not found.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})

