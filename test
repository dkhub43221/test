local locationNames = {}
for _, loc in ipairs(allLocations) do
    table.insert(locationNames, loc.Name)
end

MainTab:CreateDropdown({
    Name = "Locations",
    Options = locationNames,
    CurrentOption = {locationNames[1]},
    MultipleOptions = false,
    Flag = "MainTeleportDropdown",
    Callback = function(options)
        local selectedLocation
        for _, loc in ipairs(allLocations) do
            if loc.Name == options[1] then
                selectedLocation = loc
                break
            end
        end

        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") and selectedLocation then
            char.HumanoidRootPart.CFrame = selectedLocation.Position
        end
    end
})
