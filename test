AutofarmTab:CreateSection("Mop Autofarm")

local mopAutofarmRunning = false
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local function getSafePosition(position)
    local raycastResult = workspace:Raycast(position, Vector3.new(0, -10, 0))
    if raycastResult then
        return position + Vector3.new(0, 5, 0)
    else
        return position
    end
end

local function teleportAndFire(prompt)
    local cleanPart = prompt.Parent
    character:SetPrimaryPartCFrame(cleanPart.CFrame)
    task.wait(0.3)
    fireproximityprompt(prompt)
end

local Toggle = AutofarmTab:CreateToggle({
    Name = "Mop Autofarm",
    CurrentValue = false,
    Flag = "MopAutofarm",
    Callback = function(Value)
        if Value then
            if mopAutofarmRunning then return end
            mopAutofarmRunning = true

            local safePosition = getSafePosition(CFrame.new(-1655.081543, 3.507527, 42.390194).Position)
            character:SetPrimaryPartCFrame(CFrame.new(safePosition))
            task.wait(0.3)

            game:GetService("ReplicatedStorage").giveMop:FireServer()
            task.wait(1)

            local mop = player.Backpack:FindFirstChild("Mop")
            if mop then
                mop.Parent = character
                mop.Handle.Anchored = false
                mop.Handle.CFrame = character:WaitForChild("RightHand").CFrame
            end

            local prompts = {
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[9].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn.Dirt.Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[5].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[2].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[7].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[6].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[8].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[4].Main.ProximityPrompt,
                workspace.Cleaning_System.Dirt_Spawn:GetChildren()[3].Main.ProximityPrompt
            }

            while mopAutofarmRunning do
                for _, prompt in ipairs(prompts) do
                    teleportAndFire(prompt)
                    task.wait(0.3)
                end
            end
        else
            mopAutofarmRunning = false
        end
    end
})

AutofarmTab:CreateSection("Fake Check Autofarm")

local autofarmRunning = false

local Toggle = AutofarmTab:CreateToggle({
    Name = "Fake Checks Autofarm",
    CurrentValue = false,
    Flag = "FakeChecksAutofarm",
    Callback = function(Value)
        if Value then
            if autofarmRunning then return end
            autofarmRunning = true

            for _, v in ipairs(workspace:GetDescendants()) do
                if v:IsA("ProximityPrompt") then
                    v.HoldDuration = 0
                end
            end

            local prompts = {
                workspace.OtherItems.CheckTable.CloneC.ProximityPrompt1,
                workspace.OtherItems.CheckTable.ActivateC.ProximityPrompt2,
                workspace.OtherItems:GetChildren()[599]["Vender cc"].Vender
            }

            local cframes = {
                CFrame.new(-2405.316650, 3.407532, -42.879230),
                CFrame.new(-2454.811523, 109.807449, -218.501816),
                CFrame.new(-2450.307617, 109.807449, -218.223892),
                CFrame.new(-2358.492676, 3.535766, 131.445221)
            }

            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cframes[1]
            task.wait(3)

            while autofarmRunning do
                for i, prompt in ipairs(prompts) do
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(cframes[i + 1])
                    task.wait(0.3)
                    fireproximityprompt(prompt)
                    task.wait(1)
                end
            end
        else
            autofarmRunning = false
        end
    end
})

AutofarmTab:CreateSection("Car")

local teleporting = false

local function findPrompts(root)
    local prompts = {}
    for _, item in ipairs(root:GetDescendants()) do
        if item:IsA("ProximityPrompt") then
            table.insert(prompts, item)
        end
    end
    return prompts
end

local prompts = findPrompts(workspace.CarRobberys)
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()

local function teleportToPrompts()
    for _, prompt in ipairs(prompts) do
        if not teleporting then return end
        local pos = prompt.Parent.Position
        character:SetPrimaryPartCFrame(CFrame.new(pos))
        task.wait(1)
        fireproximityprompt(prompt)
    end
end

local Toggle = AutofarmTab:CreateToggle({
    Name = "Car Rob Autofarm",
    CurrentValue = false,
    Flag = "CarRobAutofarm",
    Callback = function(Value)
        if Value then
            teleporting = true
            while teleporting do
                teleportToPrompts()
                task.wait(1)
            end
        else
            teleporting = false
        end
    end
})

AutofarmTab:CreateSection("Grass Autofarm")

local targetCFrames = {
    CFrame.new(-1989.46448, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0),
    CFrame.new(-1981.86438, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0),
    CFrame.new(-1985.76453, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0),
    CFrame.new(-1975.26453, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0),
    CFrame.new(-1967.66443, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0),
    CFrame.new(-1971.56458, 6.79505634, 182.808807, 0, 0, 1, 0, 1, 0, -1, 0, 0)
}

local secondaryCFrame = CFrame.new(-2005.133301, 3.489641, 196.952301)

local function equipGrass()
    local bp = game.Players.LocalPlayer.Backpack
    for _, item in ipairs(bp:GetChildren()) do
        if item.Name == "Grass" then
            item.Parent = game.Players.LocalPlayer.Character
        end
    end
end

local function teleportAndFirePrompt(targetCFrame)
    local char = game.Players.LocalPlayer.Character
    char:SetPrimaryPartCFrame(targetCFrame)
    task.wait(0.1)
    for _, part in ipairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.CFrame == targetCFrame then
            local prompt = part:FindFirstChildOfClass("ProximityPrompt")
            if prompt then
                fireproximityprompt(prompt)
                break
            end
        end
    end
end

local looping = false

local Toggle = AutofarmTab:CreateToggle({
    Name = "Grass Autofarm",
    CurrentValue = false,
    Flag = "GrassAutofarm",
    Callback = function(Value)
        if Value then
            looping = true
            while looping do
                for _, cframe in ipairs(targetCFrames) do
                    equipGrass()
                    teleportAndFirePrompt(cframe)
                    task.wait(1)
                    teleportAndFirePrompt(secondaryCFrame)
                    task.wait(1)
                end
            end
        else
            looping = false
        end
    end
})

AutofarmTab:CreateSection("Other")

local instantEnabled = false
local storedDurations = {}

local Toggle = AutofarmTab:CreateToggle({
    Name = "Instant Prompts",
    Default = false,
    Callback = function(v)
        instantEnabled = v
        if v then
            for _, prompt in ipairs(workspace:GetDescendants()) do
                if prompt:IsA("ProximityPrompt") then
                    storedDurations[prompt] = prompt.HoldDuration
                    prompt.HoldDuration = 0
                end
            end
        else
            for prompt, dur in pairs(storedDurations) do
                if prompt and prompt.Parent then
                    prompt.HoldDuration = dur
                end
            end
            storedDurations = {}
        end
    end
})


