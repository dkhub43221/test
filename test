local function dupeTool(repeatCount)
    repeatCount = repeatCount or 5
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local equippedTool = character:FindFirstChildOfClass("Tool")

    if equippedTool then
        local safe = workspace:WaitForChild("Apartments"):WaitForChild("LuxuryApartment"):WaitForChild("Safe")

        for i = 1, repeatCount do
            ReplicatedStorage:WaitForChild("StoreItemSafe"):FireServer(safe)
            wait(0.1)
            ReplicatedStorage:WaitForChild("TakeItemSafe"):FireServer(equippedTool.Name, safe)
            wait(0.1)
            ReplicatedStorage:WaitForChild("StoreItemSafe"):FireServer(safe)
            wait(0.1)
            ReplicatedStorage:WaitForChild("TakeItemSafe"):FireServer(equippedTool.Name, safe)
            wait(0.1)
        end
    else
        warn("No tool equipped!")
    end
end

dupeTool(5)
