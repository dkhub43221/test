local ToggleLoop = false

GeneralTab:CreateToggle({
    Name = "Dupelacting",
    CurrentValue = false,
    Callback = function(Value)
        ToggleLoop = Value
        if Value then
            task.spawn(function()
                while ToggleLoop do

                    game:GetService("ReplicatedStorage").ATMRF:InvokeServer("withdraw", 0.5)
                    task.wait()

                    game:GetService("ReplicatedStorage").ATMRF:InvokeServer("deposit", 0.5)
                    task.wait()
