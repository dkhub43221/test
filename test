local Window = Rayfield:CreateWindow({
   Name = "DK's Hub | Streetz War 2 | V1 | " .. executor,
   Icon = nil,
   LoadingTitle = "",
   LoadingSubtitle = "",
   Theme = customTheme,
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {Enabled = false, FolderName = "BlackThemeHub", FileName = "BigHub"},
   Discord = {Enabled = true, Invite = "Fy7PanRV", RememberJoins = true},
   KeySystem = false,
   KeySettings = {Title = "X-DK | V1", Subtitle = "Enter Key Below", Note = "Buy A Key At Discord.gg/dkshub", FileName = "Key", SaveKey = true, GrabKeyFromSite = false, Key = {""}, Theme = customTheme}
})

local MainTab = Window:CreateTab("Main", "box")
MainTab:CreateSection("Bypass.lmao aka fuck abd")

local isRunning = false

MainTab:CreateToggle({
    Name = "Generate Max Money",
    CurrentValue = false,
    Flag = "AutoATM",
    Callback = function(value)
        isRunning = value
        if isRunning then
            spawn(function()
                while isRunning do
                    for i = 1, 2000 do
                        spawn(function()
                            pcall(function()
                                game:GetService("ReplicatedStorage"):WaitForChild("ATMRF"):InvokeServer("withdraw", 0.5)
                            end)
                        end)
                        spawn(function()
                            pcall(function()
                                game:GetService("ReplicatedStorage"):WaitForChild("ATMRF"):InvokeServer("deposit", 0.5)
                            end)
                        end)
                    end
                end
            end)
        end
    end
})

MainTab:CreateSection("Dupe Tools")

local amount = 1

local Slider = MainTab:CreateSlider({
    Name = "Dupe Amount",
    Range = {1, 25},
    Increment = 1,
    Suffix = "",
    CurrentValue = 1,
    Flag = "DupeAmount",
    Callback = function(Value)
        amount = Value
    end,
})

MainTab:CreateButton({
    Name = "Dupe Tool",
    Callback = function()

        local RS = game:GetService("ReplicatedStorage")
        local Players = game:GetService("Players")
        local p = Players.LocalPlayer
        local c = p.Character or p.CharacterAdded:Wait()

        local tool = c:FindFirstChildOfClass("Tool")
        if not tool then
            Rayfield:Notify({
                Title = "Error",
                Content = "No tool equipped!",
                Duration = 4,
            })
            return
        end

        local r = RS:WaitForChild("TakeItemSafe")
        local name = tool.Name

        local safes = {}
        for _, v in ipairs(workspace:GetDescendants()) do
            if v.Name:lower():find("safe") then
                safes[#safes+1] = v
            end
        end

        for _, safe in ipairs(safes) do
            for i = 1, amount do
                task.spawn(function()
                    for _ = 1, 5 do
                        pcall(r.FireServer, r, name, safe)
                    end
                end)
            end
        end

        Rayfield:Notify({
            Title = "Dupe!",
            Content = "Duping..",
            Duration = 4,
        })
    end,
})
