local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local apartmentsFolder = workspace:WaitForChild("Apartments")

for _, apartment in ipairs(apartmentsFolder:GetChildren()) do
    local safe = apartment:FindFirstChild("Safe")
    if safe then
        local storeItemSafe = ReplicatedStorage:WaitForChild("StoreItemSafe")
        storeItemSafe:FireServer(safe)
        
        local takeItemSafe = ReplicatedStorage:WaitForChild("TakeItemSafe")
        local takeArgs = {"Tec9", safe}
        
        for i = 1, 100 do
            takeItemSafe:FireServer(unpack(takeArgs))
            wait(0.3)
        end
    end
end
