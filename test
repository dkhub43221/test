local GunModifyTab = Window:CreateTab("Gun Modify", nil)
GunModifyTab:CreateSection("Gun Mods")


local AutoEnabled = false
local NoRecoilEnabled = false
local NoSpreadEnabled = false
local InfAmmoEnabled = false
local BurstEnabled = false

local player = game:GetService("Players").LocalPlayer
local backpack = player:WaitForChild("Backpack")

function ApplyGunSettings(tool)
    local settingFolder = tool:FindFirstChild("Setting")
    if not settingFolder then return end

    local module1 = settingFolder:FindFirstChild("1")
    if module1 and module1:IsA("ModuleScript") then
        local gunSettings = require(module1)
        setreadonly(gunSettings, false)

        if AutoEnabled then gunSettings.Auto = true end
        if NoSpreadEnabled then gunSettings.Spread = 0 end
        if NoRecoilEnabled then gunSettings.Recoil = 0 end
        if InfAmmoEnabled then
            gunSettings.TacticalReloadTime = 0
            gunSettings.AmmoPerMag = math.huge
            gunSettings.LimitedAmmoEnabled = false
            gunSettings.Ammo = 99999
        end

        if BurstEnabled then
            gunSettings.BurstFireEnabled = true
            gunSettings.BulletPerBurst = 5
            gunSettings.MaximumFireRate = 0.1
        end

        setreadonly(gunSettings, true)
    end
end

function ApplyToAllGuns()
    for _, tool in ipairs(backpack:GetChildren()) do
        ApplyGunSettings(tool)
    end
end

backpack.ChildAdded:Connect(function(child)
    task.wait(0.1)
    ApplyGunSettings(child)
end)

GunModifyTab:CreateToggle({
    Name = "Enable Inf Ammo",
    CurrentValue = false,
    Callback = function(v)
        InfAmmoEnabled = v
        ApplyToAllGuns()
    end
})

GunModifyTab:CreateToggle({
    Name = "Enable Full Auto",
    CurrentValue = false,
    Callback = function(v)
        AutoEnabled = v
        ApplyToAllGuns()
    end
})

GunModifyTab:CreateToggle({
    Name = "Enable No Recoil",
    CurrentValue = false,
    Callback = function(v)
        NoRecoilEnabled = v
        ApplyToAllGuns()
    end
})

GunModifyTab:CreateToggle({
    Name = "Enable No Spread",
    CurrentValue = false,
    Callback = function(v)
        NoSpreadEnabled = v
        ApplyToAllGuns()
    end
})

GunModifyTab:CreateToggle({
    Name = "Enable Burst Mode",
    CurrentValue = false,
    Callback = function(v)
        BurstEnabled = v
        ApplyToAllGuns()
    end
})


