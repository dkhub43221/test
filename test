local Window = Rayfield:CreateWindow({
   Name = "DK's Hub | Streetz War 2 | V0.0 [BETA] | " .. executor,
   Theme = customTheme,
   ConfigurationSaving = {Enabled = false},
   Discord = {Enabled = true, Invite = "Fy7PanRV", RememberJoins = true},
   KeySystem = false
})

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Backpack = LocalPlayer:WaitForChild("Backpack")

local AutofarmTab = Window:CreateTab("Autofarm", "clock-4")
AutofarmTab:CreateSection("Box Autofarm")

local boxToggle = false

AutofarmTab:CreateToggle({
    Name = "Box Autofarm",
    CurrentValue = false,
    Flag = "BoxToggle",
    Callback = function(value)
        boxToggle = value
        if value then
            task.spawn(function()
                while boxToggle do
                    local char = LocalPlayer.Character
                    if not char then continue end

                    local hrp = char:WaitForChild("HumanoidRootPart")
                    hrp.CFrame = CFrame.new(-121, 4, 301)
                    task.wait(0.2)

                    local box = workspace.Job.Box.BOX1
                    fireclickdetector(box.ClickDetector)
                    task.wait(0.2)

                    local tool = Backpack:FindFirstChild("Box")
                    if tool then char.Humanoid:EquipTool(tool) end

                    hrp.CFrame = CFrame.new(-111, 4, 146)
                    task.wait(0.5)
                end
            end)
        end
    end
})

AutofarmTab:CreateSection("Mop Autofarm")

local giveMop = ReplicatedStorage:FindFirstChild("GiveMop")
local cleaningEnabled = false

local function equipMop()
    local mop = Backpack:FindFirstChild("Mop")
    if mop then mop.Parent = LocalPlayer.Character end
end

AutofarmTab:CreateToggle({
    Name = "Mop Autofarm",
    CurrentValue = false,
    Flag = "CleanToggle",
    Callback = function(state)
        cleaningEnabled = state
        if not state then return end

        task.spawn(function()
            while cleaningEnabled do
                for _, part in ipairs(workspace.CleanPart:GetChildren()) do
                    if not cleaningEnabled then break end

                    if giveMop then
                        if giveMop:IsA("RemoteEvent") then
                            giveMop:FireServer()
                        else
                            giveMop:InvokeServer()
                        end
                        task.wait(1)
                        equipMop()
                    end

                    local pos = part:IsA("BasePart") and part.Position or (part.PrimaryPart and part.PrimaryPart.Position)
                    if pos then
                        LocalPlayer.Character:MoveTo(pos)
                        task.wait(3)
                    end

                    for _, prompt in ipairs(part:GetDescendants()) do
                        if prompt:IsA("ProximityPrompt") then
                            fireproximityprompt(prompt)
                        end
                    end
                end
            end
        end)
    end
})

local CardTab = Window:CreateTab("Card", "credit-card")
CardTab:CreateSection("Autofarm Card")

local autoEquipLaptop = false

local function equipLaptop()
    local tool = Backpack:FindFirstChild("Laptop")
    if tool and LocalPlayer.Character then tool.Parent = LocalPlayer.Character end
end

RunService.RenderStepped:Connect(function()
    if autoEquipLaptop then equipLaptop() end
end)

Backpack.ChildAdded:Connect(function(tool)
    if autoEquipLaptop and tool.Name == "Laptop" then
        equipLaptop()
    end
end)

CardTab:CreateToggle({
    Name = "Auto Equip Laptop",
    CurrentValue = false,
    Callback = function(state)
        autoEquipLaptop = state
        if state then equipLaptop() end
    end
})

_G.autofarm = false
local farmDelay = 0.01

CardTab:CreateToggle({
    Name = "Start Card Autofarm",
    CurrentValue = false,
    Flag = "AutoFarmToggle",
    Callback = function(state)
        _G.autofarm = state
        if not state then return end

        local hrp = LocalPlayer.Character:WaitForChild("HumanoidRootPart")
        hrp.CFrame = CFrame.new(226, 4, -540)

        local punch = ReplicatedStorage.Assets.Other.GiverPunchmade
        local args1 = {true, "NEW123"}
        local args2 = {false, "NEW123"}

        task.spawn(function()
            while _G.autofarm do
                punch:InvokeServer(unpack(args1))
                task.wait(farmDelay)
                punch:InvokeServer(unpack(args2))
                task.wait(farmDelay)
            end
        end)
    end
})

local TeleportationTab = Window:CreateTab("Teleportation", "map-pin")
TeleportationTab:CreateSection("Teleports")

local function createTeleportDropdown(tab, name, list)
    local names = {}
    for _, v in ipairs(list) do table.insert(names, v.Name) end

    tab:CreateDropdown({
        Name = name,
        Options = names,
        CurrentOption = {names[1]},
        MultipleOptions = false,
        Callback = function(option)
            local selected
            for _, v in ipairs(list) do
                if v.Name == option[1] then selected = v end
            end

            local char = LocalPlayer.Character
            if char and selected then
                char.HumanoidRootPart.CFrame = selected.Position
            end
        end
    })
end

local PlayerTab = Window:CreateTab("Player", "wand")
PlayerTab:CreateSection("Speed")

local defaultWalkSpeed = 16
local currentWalkSpeed = 16

local SpeedToggle = PlayerTab:CreateToggle({
    Name = "Enable Speed",
    CurrentValue = false,
    Callback = function(state)
        local char = LocalPlayer.Character
        if not char then return end

        if char:FindFirstChild("CharacterScripts") then
            char.CharacterScripts.Enabled = not state
        end

        if char:FindFirstChild("Humanoid") then
            char.Humanoid.WalkSpeed = state and currentWalkSpeed or defaultWalkSpeed
        end
    end
})

PlayerTab:CreateInput({
    Name = "Speed (16-150)",
    PlaceholderText = "Enter WalkSpeed",
    Callback = function(text)
        local num = tonumber(text)
        if not num then return end

        currentWalkSpeed = math.clamp(num, 16, 150)
        local char = LocalPlayer.Character

        if char and SpeedToggle.CurrentValue then
            char.Humanoid.WalkSpeed = currentWalkSpeed
        end
    end
})
