Section = Tab:AddSection({
	Name = "Mop Autofarm"
})




local mopAutofarmRunning = false
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

function getSafePosition(position)
    local raycastResult = workspace:Raycast(position, Vector3.new(0, -10, 0))
    if raycastResult then
        return position + Vector3.new(0, 5, 0)
    else
        return position
    end
end

function teleportAndFire(prompt)
    local cleanPart = prompt.Parent
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(cleanPart.CFrame)
    wait(0.3)
    fireproximityprompt(prompt)
end

Tab:AddToggle({
    Name = "Mop Autofarm",
    Default = false,
    Callback = function(Value)
        if Value then
            if not mopAutofarmRunning then
                mopAutofarmRunning = true

                local safePosition = getSafePosition(CFrame.new(-1655.081543, 3.507527, 42.390194).Position)
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(safePosition))
                wait(0.3)
                
                game:GetService("ReplicatedStorage").giveMop:FireServer()
                wait(1)

                local mop = player.Backpack:FindFirstChild("Mop")
                if mop then
                    mop.Parent = character
                    mop.Handle.Anchored = false
                    mop.Handle.CFrame = character:WaitForChild("RightHand").CFrame
                end

                local prompts = {
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[9].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn.Dirt.Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[5].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[2].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[7].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[6].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[8].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[4].Main.ProximityPrompt,
                    workspace.Cleaning_System.Dirt_Spawn:GetChildren()[3].Main.ProximityPrompt
                }

                while mopAutofarmRunning do
                    for _, prompt in ipairs(prompts) do
                        local cleanPart = prompt.Parent
                        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(cleanPart.CFrame)
                        wait(0.3)
                        fireproximityprompt(prompt)
                        wait(0.3)
                    end
                end

                mopAutofarmRunning = false
            end
        else
            mopAutofarmRunning = false
        end
    end
})





