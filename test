local timeLabel = Tab:AddLabel("Time: --:--:-- AM/PM")

local function updateTime()
    while true do
        local currentTime = os.date("%I:%M:%S %p")
        timeLabel:Set("Time: " .. currentTime)
        wait(1)
    end
end

spawn(updateTime)











