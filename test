local Divider = MainTab:CreateDivider()

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local enabled = false

local function getHRP()
    local char = player.Character or player.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

local function collectAllCash()
    local cashFolder = workspace:FindFirstChild("CashF")
    if not cashFolder then return end

    local hrp = getHRP()

    for _, cash in ipairs(cashFolder:GetChildren()) do
        if not enabled then break end

        local targetPart = cash:IsA("BasePart") and cash or cash:FindFirstChildWhichIsA("BasePart")
        if targetPart then
            hrp.CFrame = targetPart.CFrame + Vector3.new(0, 1.5, 0)
            task.wait(0.05)

            for _, obj in ipairs(cash:GetDescendants()) do
                if obj:IsA("ProximityPrompt") then
                    fireproximityprompt(obj)
                    task.wait(0.03)
                end
            end

            task.wait(0.1)
        end
    end
end

task.spawn(function()
    while true do
        if enabled then
            collectAllCash()
        else
            task.wait(0.1)
        end
        task.wait(0.1)
    end
end)

